set maxwidth 80
set cmitmode patch
set wrapcomment none
set autoselect 1
set autosellen 40
set showneartags 1
set hideremotes 1
set showlocalchanges 1
set datetimeformat {%Y-%m-%d %H:%M:%S}
set limitdiffs 1
set uicolor grey85
set want_ttk 1
set bgcolor white
set fgcolor black
set colors {green red blue magenta darkgrey brown orange}
